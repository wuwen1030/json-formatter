{
  "name": "json-formatter",
  "displayName": "JSON Formatter",
  "description": "Format JSON with minify and beautify commands",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-formatter.minify",
        "title": "JSON: Minify"
      },
      {
        "command": "json-formatter.beautify",
        "title": "JSON: Beautify"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == json || editorLangId == jsonc",
          "command": "json-formatter.minify",
          "group": "1_modification"
        },
        {
          "when": "editorLangId == json || editorLangId == jsonc",
          "command": "json-formatter.beautify",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "typescript": "^4.7.4",
    "vsce": "^2.15.0"
  },
  "keywords": [
    "json",
    "formatter",
    "minify",
    "beautify"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
